{
	"useTabs": true,
	"plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		},
		{
			"files": ["*.md", "*.mdx"],
			"options": {
				"printWidth": 65,
				"useTabs": false,
				"tabWidth": 2
			}
		}
	]
}
